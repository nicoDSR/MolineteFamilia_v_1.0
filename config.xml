<?xml version='1.0' encoding='utf-8'?>
<widget id="com.colegiodelosmolinos.molineteDocente" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Molinete Familia</name>
    <description>
        Aplicaci√≥n del proyecto Molinete, para uso de las familias.
    </description>
    <author email="colegiodelosmolinos@gmail.com" href="http://colegiodelosmolinos.edu.uy">
        Colegio De los Molinos
    </author>
    <content src="index.html" />
    <preference name="permissions" value="none" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="android-installLocation" value="auto" />
    <plugin name="org.apache.cordova.camera" />
    <plugin name="org.apache.cordova.media-capture" />
    <plugin name="org.apache.cordova.console" />
    <plugin name="org.apache.cordova.device" />
    <plugin name="org.apache.cordova.device-orientation" />
    <plugin name="org.apache.cordova.dialogs" />
    <plugin name="org.apache.cordova.file" />
    <plugin name="org.apache.cordova.file-transfer" />
    <plugin name="org.apache.cordova.media" />
    <plugin name="org.apache.cordova.network-information" />
    <plugin name="org.apache.cordova.splashscreen" />
    <plugin name="org.apache.cordova.vibration" />
    <icon src="www/icon.png" />
    <icon platform="android" qualifier="ldpi" src="www/res/icon/android/icon-36-ldpi.png" />
    <icon platform="android" qualifier="mdpi" src="www/res/icon/android/icon-48-mdpi.png" />
    <icon platform="android" qualifier="hdpi" src="www/res/icon/android/icon-72-hdpi.png" />
    <icon platform="android" qualifier="xhdpi" src="www/res/icon/android/icon-96-xhdpi.png" />
    <icon platform="blackberry" src="www/res/icon/blackberry/icon-80.png" />
    <icon platform="blackberry" state="hover" src="www/res/icon/blackberry/icon-80.png" />
    <icon platform="ios" height="57" src="www/res/icon/ios/icon-57.png" width="57" />
    <icon platform="ios" height="72" src="www/res/icon/ios/icon-72.png" width="72" />
    <icon platform="ios" height="114" src="www/res/icon/ios/icon-57-2x.png" width="114" />
    <icon platform="ios" height="144" src="www/res/icon/ios/icon-72-2x.png" width="144" />
    <icon platform="webos" src="www/res/icon/webos/icon-64.png" />
    <icon platform="winphone" src="www/res/icon/windows-phone/icon-48.png" />
    <platform name="android">
    <splash qualifier="port-ldpi" src="www/res/screen/android/screen-ldpi-portrait.png" />
    <splash qualifier="port-mdpi" src="www/res/screen/android/screen-mdpi-portrait.png" />
    <splash qualifier="port-hdpi" src="www/res/screen/android/screen-hdpi-portrait.png" />
    <splash qualifier="port-xhdpi" src="www/res/screen/android/screen-xhdpi-portrait.png" />
    <splash qualifier="land-ldpi" src="www/res/screen/android/screen-ldpi-landscape.png" />
    <splash qualifier="land-mdpi" src="www/res/screen/android/screen-mdpi-landscape.png" />
    <splash qualifier="land-hdpi" src="www/res/screen/android/screen-hdpi-landscape.png" />
    <splash qualifier="land-xhdpi" src="www/res/screen/android/screen-xhdpi-landscape.png" />
    </platform>    
    <platform name="ios">
    <splash src="www/res/screen/ios/screen-iphone-portrait.png" width="320" height="480"/>
    <splash src="rwww/res/screen/ios/screen-iphone-portrait-2x.png" width="640" height="960"/>
    <splash src="www/res/screen/ios/screen-iphone-portrait-568h-2x.png" width="640" height="1136"/> 
    <splash src="www/res/screen/ios/screen-ipad-portrait.png" width="768" height="1024"/>   
    <splash src="www/res/screen/ios/screen-ipad-landscape.png" width="1024" height="768"/>    
    <splash src="www/res/screen/ios/screen-ipad-portrait-2x.png" width="1536" height="2048"/>
    <splash src="www/res/screen/ios/screen-iphone-portrait-568h-2x.png" width="750" height="1334"/>
    <splash src="www/res/screen/ios/screen-ipad-landscape-2x.png" width="2048" height="1536"/>
    <splash src="www/res/screen/ios/screen-ipad-portrait-2x.png" width="1242" height="2208"/>
    <splash src="www/res/screen/ios/screen-ipad-landscape-2x.png" width="2208" height="1242"/>
    </platform>    
    <icon platform="winphone" role="background" src="www/res/icon/windows-phone/icon-173-tile.png" />
    <splash platform="blackberry" src="www/res/screen/blackberry/screen-225.png" />
    <splash platform="winphone" src="www/res/screen/windows-phone/screen-portrait.jpg" />
    <access origin="*" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
